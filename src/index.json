{
	"env": {
		"browser": true,
		"es2022": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
    "plugin:promise/recommended",
		"plugin:security/recommended",
		"./json",
		"./jsdoc"
	],
	"plugins": [
		"simple-import-sort",
		"unicorn"
	],
	"rules": {
		"simple-import-sort/imports": "error",
		"simple-import-sort/exports": "error"
	}
}
